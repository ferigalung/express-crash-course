{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": false
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-unused-vars": "warn",
        "no-console": "warn"
    }
}
